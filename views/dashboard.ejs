        </tr>
      </thead>
      <tbody>
        <% if (users && users.length) { %>
          <% users.forEach(user => { %>
            <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.userType %></td>
            </tr>
          <% }); %>
        <% } else { %>
          <tr>
            <td colspan="3">No registered accounts found.</td>
          </tr>
        <% } %>

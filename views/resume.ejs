<head>
    <title>Resume</title>
  </head>
  <body>
    <h1>Resume</h1>
    <h2>Name: <%= name %></h2>
    <h2>Email: <%= email %></h2>
    <h2>Age: <%= age %></h2>
    <h2>Education:</h2>
    <ul>
      <% education.forEach(function(education) { %>
        <li><%= education %></li>
      <% }) %>
    </ul>
    <h2>Experience:</h2>
    <ul>
      <% experience.forEach(function(experience) { %>
        <li><%= experience %></li>
      <% }) %>
    </ul>
    <h2>Programming Language:</h2>
    <ul>
      <% language.forEach(function(language) { %>
        <li><%= language %></li>
      <% }) %>
    </ul>
  </body>
  